from(db:"testdb")
  |> range(start:2018-05-22T19:53:26Z)
  |> group(by: ["_measurement"])
  |> window(every: 1s, start: 2,  ignoreGlobalBounds: true)
  |> mean(timeSrc: "_start")
  |> window(every: inf, ignoreGlobalBounds: true)
  |> map(fn: (r) => {_time: r._time, mean: r._value})
  |> yield(name:"0")
