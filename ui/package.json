    "react-grid-layout": "^0.16.6",
    "react-markdown": "^4.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-scrollbars-custom": "^4.0.0-alpha.8",
    "react-tooltip": "^3.2.1",
    "react-virtualized": "^9.18.5",
    "redux": "^4.0.0",
