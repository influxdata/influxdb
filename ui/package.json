    "parcel": "^1.12.3",
    "prettier": "^1.16.4",
    "react-testing-library": "^5.4.4",
    "sass": "^1.15.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslib": "^1.9.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.0"
  },
  "dependencies": {
    "@influxdata/clockface": "0.0.13",
