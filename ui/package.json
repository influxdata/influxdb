    "build:dev": "webpack --config ./webpack/dev.config.js",
    "build:vendor": "webpack --config webpack/vendor.config.js",
    "start":
      "yarn run clean && yarn run build:vendor && webpack --watch --config ./webpack/dev.config.js --progress",
