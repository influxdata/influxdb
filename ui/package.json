    "@influxdata/influxdb-templates": "0.2.0",
    "@influxdata/react-custom-scrollbars": "4.3.8",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^4.0.2",
    "calculate-size": "^1.1.1",
