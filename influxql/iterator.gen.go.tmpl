type {{$k.name}}MergeIterator struct {
	inputs []{{$k.Name}}Iterator
	heap   *{{$k.name}}MergeHeap
	init   bool

	// Current iterator and window.
	curr   *{{$k.name}}MergeHeapItem
	window struct {
