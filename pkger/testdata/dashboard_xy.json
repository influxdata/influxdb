{
  "apiVersion": "0.1.0",
  "kind": "Package",
  "meta": {
    "pkgName": "pkg_name",
    "pkgVersion": "1",
    "description": "pack description"
  },
  "spec": {
    "resources": [
      {
        "kind": "Dashboard",
        "name": "dash_1",
        "description": "desc1",
        "charts": [
          {
            "kind": "XY",
            "name": "xy chart",
            "prefix": "sumtin",
            "note": "xy chart note",
            "noteOnEmpty": true,
            "xPos": 1,
            "yPos": 2,
            "width": 6,
            "height": 3,
            "decimalPlaces": 1,
            "shade": true,
            "xColumn": "_time",
            "yColumn": "_value",
            "legend": {},
            "queries": [
              {
                "query": "from(bucket: v.bucket)  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)  |> filter(fn: (r) => r._measurement == \"boltdb_writes_total\")  |> filter(fn: (r) => r._field == \"counter\")"
              }
            ],
            "colors": [
              {
                "name": "laser",
                "type": "scale",
                "hex": "#8F8AF4",
                "value": 3
              }
            ],
            "axes":[
              {
                "name": "x",
                "label": "x_label",
                "prefix": "x_prefix",
                "suffix": "x_suffix",
                "base": "10",
                "scale": "linear"
              },
              {
                "name": "y",
                "label": "y_label",
                "prefix": "y_prefix",
                "suffix": "y_suffix",
                "base": "10",
                "scale": "linear"
              }
            ],
            "geom": "line"
          }
        ]
      }
    ]
  }
}
